{
  "manifest_version": 3,
  "name": "Lawbrokr. Chrome",
  "version": "1.0",
  "action": {
    "default_title": "Open Lawbrokr",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["sidePanel"]
}
